include ./includes/header.pug
    
.container
  h1=name
  p='Tree at commit ' + hash + '.'
  table(class='table table-striped table-hover table-condensed')
    thead
      tr
        th Path
    tbody
      each row in tree
        tr
          td: a(href='/blog/' + name + '/' + hash + '/' + row.pathURI)=row.path
        
include ./includes/footer.pug  