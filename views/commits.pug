include ./includes/header.pug
    
.container
  h1=name
  p=path
  table(class='table table-striped table-hover table-condensed')
    thead
      tr
        th Author
        th Commit
        th Message
        th Date
    tbody
      each row in all
        tr
          td=row.author_name
          td: a(href='/commits/' + name + '/' + row.hash)=row.hash
          td=row.message
          td=row.date
        
include ./includes/footer.pug  